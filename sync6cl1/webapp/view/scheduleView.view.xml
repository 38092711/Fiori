<mvc:View controllerName="sync6.cl1.sync6cl1.controller.scheduleView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
	xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core">
    <Page id="page" title="{i18n>title}">
     <Panel >
            <VBox >
                <t:Table
                    	rows="{/scheduleSet}"
                        selectionMode="None"
                        paste="onPaste"
                        ariaLabelledBy="Flight schedule">

                        <t:extension>
					        <OverflowToolbar style="Clear">
						      <Title id="Flight" text="Flight schedule"/>
					        </OverflowToolbar>
				        </t:extension>

					    <t:columns >
                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Carrid/@sap:label}" />
						    <t:template>
							    <Text text="{Carrid}" wrapping="false" />
						    </t:template>
					        </t:Column>

                             <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Connid/@sap:label}" />
						    <t:template>
							    <Text text="{Connid}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Countryfr/@sap:label}" />
						    <t:template>
							    <Text text="{Countryfr}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Cityfrom/@sap:label}" />
						    <t:template>
							    <Text text="{Cityfrom}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Airpfrom/@sap:label}" />
						    <t:template>
							    <Text text="{Airpfrom}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Countryto/@sap:label}" />
						    <t:template>
							    <Text text="{Countryto}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Cityto/@sap:label}" />
						    <t:template>
							    <Text text="{Cityto}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Airpto/@sap:label}" />
						    <t:template>
							    <Text text="{Airpto}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Fltime/@sap:label}" />
						    <t:template>
							    <Text text="{Fltime}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Deptime/@sap:label}" />
						    <t:template>
							    <Text text="{Deptime}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Arrtime/@sap:label}" />
						    <t:template>
							    <Text text="{Arrtime}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Distance/@sap:label}" />
						    <t:template>
							    <Text text="{Distance}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Distid/@sap:label}" />
						    <t:template>
							    <Text text="{Distid}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Fltype/@sap:label}" />
						    <t:template>
							    <Text text="{Fltype}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            <t:Column width="11rem">
						    <Label text="{/#scheduleSet/Period/@sap:label}" />
						    <t:template>
							    <Text text="{Period}" wrapping="false" />
						    </t:template>
					        </t:Column>

                            
					    </t:columns>
                </t:Table>
            </VBox>
        </Panel>
    </Page>
</mvc:View>